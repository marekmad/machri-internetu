# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Registration.index()

# Signup
GET     /create                     controllers.Registration.create()
POST    /save                    	controllers.Registration.save()

# Authentification
GET		/authentification			controllers.Registration.authentification()

#Quiz over
GET		/quizOver					controllers.Registration.quizOver()

#Round over
GET		/roundOver					controllers.Registration.roundOver()

#Quiz
GET		/quiz						controllers.Registration.quiz()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
